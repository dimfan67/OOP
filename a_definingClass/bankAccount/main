package OOP.a_definingClass.bankAccount;

public class main {
    public static void main(String[] args) {
BankAccount account = new BankAccount();
account.setId( 1 );
account.deposit( 50 );
        System.out.println(account.getBalance());
account.withdraw( 100 );
        System.out.println(account.getBalance());
        System.out.println(account);
    }

}
