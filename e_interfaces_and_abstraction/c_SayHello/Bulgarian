package OOP.e_interfaces_and_abstraction.c_SayHello;

public class Bulgarian implements Person{
  private String name;

    public Bulgarian(String name) {
        this.name = name;
    }

    @Override
    public String getName() {
        return this.name;
    }

    @Override
    public String seyHello() {
        return "Здравей";
    }
}
